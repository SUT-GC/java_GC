package Test2;

public class ChaseMethod {
	private int[][] a;
	private int[] x;
	private int[] d;
	private int[] y;
	private int[] u;
	private int[] c;
	private int[] l;
	public ChaseMethod(int[][] a, int[] d, int numofx){
		this.a = a;
		this.d = d;
		x = new int[numofx];
		y = new int[d.length];
		c = new int[d.length];
		l = new int[d.length];
	}
	
	public int[] getX(){
		
		return x;
	}
}
