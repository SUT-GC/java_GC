enum Season{
	RED("红色",1),GREEN("绿色",2);
	private final int a = 3;
	private String name;
	private int index;
	private Season(String name, int index){
		this.name =name;
		this.index = index;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getIndex() {
		return index;
	}
	public void setIndex(int index) {
		this.index = index;
	}
	public String toString(){
		return "name = "+this.getName()+", index = "+this.getIndex()+" ;
	}
	
}

class FinalTest{
	public static final int a = 2;
}

public class EnumTest {
	public static void main(String[] args) {
		
		System.out.println(Season.RED);
		System.out.println(Season.RED.getName());
		
		System.out.println(FinalTest.a);
	}
}
