package Test2;

import java.util.Arrays;

public class ThreeSpline {
	private double[] x;
	private double[] y;
	private double[] m;
	private double[] d;
	private double[] u;
	private double[] w;
	private double[] l;
	private double[] z;
	private double[] r;
	private double[] h;
	private double p1;
	private double p2;
	private double p3;
	private double p4;
	private int n;;

	public ThreeSpline(double[] xx, double[] yy){
		h=new double[10];
		x=new double[10];
		y=new double[10];
		m=new double[10];//M
		d=new double[10];
		u=new double[10];//μ
		r=new double[10];
		w=new double[10];//追赶法中的u
		z=new double[10];//追赶法中的y
		l=new double[10];//追赶法中的l
		p1=0;p2=0;p3=0;p4=0;
		n=0;		
	}
}	
