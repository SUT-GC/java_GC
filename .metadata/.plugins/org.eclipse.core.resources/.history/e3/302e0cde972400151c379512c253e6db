package Test2;

public class ThreeSpline {
	private double[] x;
	private double[] y;
	private double[] h;
	private double[] u;
	private double[] r;
	public ThreeSpline(double[] xx, double[] yy){
		this.x = xx;
		this.y = yy;
		h = new double[x.length-1];
		u = new double[h.length-1];
		r = new double[h.length-1];
	}
}	
