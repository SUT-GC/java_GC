package curriculumdesign.cd7;

import java.util.ArrayDeque;
import java.util.Vector;

public class PageRep {
	private int[] seqs;
	private int[] timeshort;
	private int[] mem;
	private int[] timelong;
	public PageRep(int[] seqs, int memorysize){
		this.seqs = seqs;
		timeshort = new int[memorysize];
		timelong = new int[memorysize];
		mem = new int[memorysize];
		for(int i = 0; i < timeshort.length; i++){
			timeshort[i] = 0;
			timelong[i] = 0;
		}
		for(int i = 0; i < memorysize; i++){
			mem[i] = -1;
		}
	}
	boolean find(int num, int[] a){
		for(int i = 0; i < a.length; i++){
			if(a[i] == num){
				return true;
			}
		}
		return false;
	}
	public void FIFO(){
		int seat = 0;
		for(int i = 0; i < seqs.length; i++){
			if(!find(seqs[i], mem)){
				mem[seat] = seqs[i];
				seat++;
				seat = seat % mem.length;
			}
		}
	}
}
