enum Season{
	RED("红色",1),GREEN("绿色",2);
	
	private String name;
	private int index;
	private Season(String name, int index){
		this.name =name;
		this.index = index;
	}
	
}

public class EnumTest {
	public static void main(String[] args) {
		Season s = Season.SPRING;
		switch(s){
		case SPRING:{
			System.out.println("Spring");
			break;
		}
		}
	}
}
